<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="AxisFrameView">

        <attr name="xAxisVisible" format="boolean|reference"/>
        <attr name="xAxisLineVisible" format="boolean|reference"/>
        <attr name="xAxisLineColor" format="color|reference"/>
        <attr name="xAxisLineWidth" format="dimension|reference"/>
        <attr name="xAxisTextSize" format="dimension|reference"/>
        <attr name="xAxisTextColor" format="color|reference"/>
        <attr name="xAxisSpacing" format="dimension|reference"/>
        <attr name="xAxisValueLineColor" format="color|reference"/>
        <attr name="xAxisValueLineWidth" format="dimension|reference"/>
        <attr name="xAxisValueLineVisible" format="boolean|reference"/>
        <attr name="xAxisValueLineStyle" format="enum">
            <enum name="normal" value="0"/>
            <enum name="dash" value="1"/>
        </attr>
        <attr name="xAxisValueLineDashWidth" format="dimension|reference"/>
        <attr name="xAxisValueLineDashGap" format="dimension|reference"/>

        <attr name="yAxisVisible" format="boolean|reference"/>
        <attr name="yAxisLineVisible" format="boolean|reference"/>
        <attr name="yAxisLineColor" format="color|reference"/>
        <attr name="yAxisLineWidth" format="dimension|reference"/>
        <attr name="yAxisTextSize" format="dimension|reference"/>
        <attr name="yAxisTextColor" format="color|reference"/>
        <attr name="isYAxisInside" format="boolean|reference"/>
        <attr name="yAxisSpacing" format="dimension|reference"/>
        <attr name="yAxisValueLineColor" format="color|reference"/>
        <attr name="yAxisValueLineStyle" format="enum">
            <enum name="normal" value="0"/>
            <enum name="dash" value="1"/>
        </attr>
        <attr name="yAxisValueLineWidth" format="dimension|reference"/>
        <attr name="yAxisValueLineVisible" format="boolean|reference"/>
        <attr name="yAxisValueLineDashWidth" format="dimension|reference"/>
        <attr name="yAxisValueLineDashGap" format="dimension|reference"/>

        <attr name="dragEnable" format="boolean"/>
        <attr name="indicatorLineColor" format="color"/>
        <attr name="indicatorLineWidth" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="HistogramView">
        <attr name="minRectSpacing" format="dimension|reference"/>
        <attr name="maxRectWidth" format="dimension|reference"/>
    </declare-styleable>

    <declare-styleable name="RingChartView">
        <attr name="ringWidth" format="dimension|reference"/>
        <attr name="ringTextSize" format="dimension|reference"/>
        <attr name="ringTextVisible" format="boolean|reference"/>
    </declare-styleable>

</resources>